{
  "builders": [{
    "type": "amazon-ebs",
    "region": "us-west-2",
    "source_ami": "ami-972d4bf7",
    "instance_type": "t2.micro",
    "ssh_username": "ubuntu",
    "ami_name": "packer-ami {{timestamp}}"
  }],
  "provisioners": [{
    "type": "shell",
    "scripts": [  "setup_script.sh"   ],
    "execute_command": "{{ .Vars }} sudo -E sh '{{ .Path }}'",
    "pause_before": "10s"
  }]
}
